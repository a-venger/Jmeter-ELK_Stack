{
  "name": "vega-tooltip",
  "version": "0.9.14",
  "description": "A tooltip plugin for Vega-Lite and Vega visualizations.",
  "main": "build/vega-tooltip.js",
  "module": "build/index.js",
  "unpkg": "build/vega-tooltip.min.js",
  "jsdelivr": "build/vega-tooltip.min.js",
  "typings": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/vega/vega-tooltip.git"
  },
  "keywords": [
    "vega-lite",
    "vega",
    "tooltip"
  ],
  "author": "Interactive Data Lab",
  "collaborators": [
    "Dominik Moritz",
    "Sira Horradarn",
    "Zening Qu",
    "Kanit Wongsuphasawat",
    "Yuri Astrakhan",
    "Jeffrey Heer"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/vega/vega-tooltip/issues"
  },
  "homepage": "https://github.com/vega/vega-tooltip#readme",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf build && mkdir build",
    "data": "rsync -r node_modules/vega-datasets/data/* data",
    "deploy:gh": "scripts/deploy-gh.sh",
    "format": "tslint -p . --fix && prettier --write 'src/**/*.ts'",
    "lint": "tslint -p . && prettier --list-different 'src/**/*.ts'",
    "postbuild": "uglifyjs build/vega-tooltip.js -c -m -o build/vega-tooltip.min.js",
    "prebuild": "npm run data && npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint",
    "start": "concurrently --kill-others -n Server,Typescript,Rollup 'browser-sync start -s -f build *.html' 'tsc -w' 'rollup -c -w'"
  },
  "devDependencies": {
    "browser-sync": "^2.23.7",
    "concurrently": "^3.5.1",
    "prettier": "^1.12.1",
    "rollup": "^0.58.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3",
    "uglify-js": "^3.3.22",
    "vega-datasets": "^1.18.0",
    "vega-typings": "^0.2.15"
  },
  "dependencies": {
    "json-stringify-safe": "^5.0.1",
    "vega-util": "^1.7.0"
  }
}
