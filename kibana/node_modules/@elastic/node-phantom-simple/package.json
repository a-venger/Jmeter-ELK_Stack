    "test-slimer": "ENGINE=slimerjs ./node_modules/.bin/mocha"
  },
  "devDependencies": {
    "eslint": "0.24.1",
    "eslint-plugin-nodeca": "^1.0.0",
    "mocha": "^2.2.5",
