    "path-to-regexp": "^1.7.0"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-cli": "^6.1.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.1.2",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.6",
    "browserify": "^13.1.0",
